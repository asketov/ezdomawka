@model IEnumerable<BLL.Models.ViewModels.BanVm>

<div id="bans">
    @if (Model.Any())
    {
        <div class="pt-1 col-12">
            @foreach (var el in Model)
            {
                <partial model="@el" name="Partials/_BanCard" />
            }
        </div>
    }
    else
    {
        <div class="d-flex justify-content-center pt-4">
            <h2 class="text-center">Пользователь никогда не был забанен.</h2>
        </div>
    }
</div>